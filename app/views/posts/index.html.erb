
  <tbody>
    <% @posts.each do |post| %>
    <div class="col-lg-3 col-md-6 mb-4">
      <div class="card h-100">
        <div class="card-body">
          <h4 class="card-title"> <%= post.title %> </h4>
          <p class="card-text"><%= post.body %></p>
        </div>
        <div class="card-footer">
          <a href="#" class="btn btn-primary">Find Out More!</a>
        </div>
      </div>
    </div>
    <% end %>
  </tbody>

<br>
